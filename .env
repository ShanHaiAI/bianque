# .env
# -----------------------------------------------------------------------------
# LLM 服务相关配置
QWEN_API_KEY=your_qwen_api_key
QWEN_API_URL=https://api.qwen.example/llm

DEEPSEEK_API_KEY=your_deepseek_api_key
DEEPSEEK_API_URL=https://api.deepseek.example/llm

# 应用ID (必须为webapi类型应用，并印刷文字识别服务，参考帖子如何创建一个webapi应用：http://bbs.xfyun.cn/forum.php?mod=viewthread&tid=36481)
XFYUN_APPID="a6df95c8"
# 接口密钥(webapi类型应用开通印刷文字识别服务后，控制台--我的应用---印刷文字识别---服务的apikey)
XFYUN_API_KEY="4fb35f39a30cfdc8e2032f0998b27bcc"

# -----------------------------------------------------------------------------
# MCP 平台配置（深度接入：包括数据查询、预约信息等接口）
MCP_API_KEY=your_mcp_api_key
MCP_API_URL=https://api.mcp.example/query
# 如有其他 MCP 扩展配置，可增加如下：
MCP_EXTRA_CONFIG=extra_value

# -----------------------------------------------------------------------------
# 向量知识库 (Milvus) 配置
MILVUS_HOST=localhost
MILVUS_PORT=19530

# -----------------------------------------------------------------------------
# 日志相关配置
LOG_LEVEL=INFO

# -----------------------------------------------------------------------------
# 选择使用哪种 LLM 服务: qwen 或 deepseek
SELECTED_LLM=qwen
